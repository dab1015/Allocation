        function getValues() {


            var unitqty = $("#Quantity").val();
            var qtotal = $('#pallettotal').val();
            var ispk = $('#unitpercase').val();
            var tie = $('#pallettie').val();
            var hie = $('#pallethigh').val();

            var palletqty = unitqty / (tie * hie * ispk);
            $("#PQty").val(parseInt(palletqty));

            if ($('#Quantity').val() == 0) {
                $('#PQty').val(0);
            }

            else {

                $("#PQty").val(parseInt(palletqty));
            }
